
<div id="container">
	 <div class="item">
<li id="<%= feed_me_item.id %>">
  <%= link_to gravatar_for(feed_me_item.user), feed_me_item.user %>
  <span class="user">
    <%= link_to feed_me_item.user.name, feed_me_item.user %>
  </span>
  <span class="title"><%= feed_me_item.content %></span>

   <span class="img"><%= link_to  image_tag(feed_me_item.image_url), feed_me_item.image_url%></span>


  <span class="timestamp">
    Posted <%= time_ago_in_words(feed_me_item.created_at) %> ago.

  </span>
  <% if current_user?(feed_me_item.user) %>
    <%= link_to "delete", feed_me_item, method: :delete,
                                     data: { confirm: "You sure?" },
                                     title: feed_me_item.content %>
  <% end %>
  
</li>
</div>
</div>

