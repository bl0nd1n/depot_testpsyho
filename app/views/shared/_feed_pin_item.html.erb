
<div id="container">
	 <div class="item">
<li id="<%= feed_pin_item.id %>">
  <%= link_to gravatar_for(feed_pin_item.user), feed_pin_item.user %>
  <span class="user">
    <%= link_to feed_pin_item.user.name, feed_pin_item.user %>
  </span>
  <span class="title"><%= feed_pin_item.title %></span>

   <span class="img"><%= link_to  image_tag(feed_pin_item.image), feed_pin_item.image.url%></span>


  <span class="timestamp">
    Posted <%= time_ago_in_words(feed_pin_item.created_at) %> ago.

  </span>
  <% if current_user?(feed_pin_item.user) %>
    <%= link_to "delete", feed_pin_item, method: :delete,
                                     data: { confirm: "You sure?" },
                                     title: feed_pin_item.title %>
  <% end %>
  
</li>
</div>
</div>

